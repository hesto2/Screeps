module.exports = function (creep) {

   var couriers = creep.pos.findClosest(FIND_MY_CREEPS, {filter: function(object) {return (object.memory.role == 'courier') }});
    var sources = creep.pos.findClosest(FIND_SOURCES);
//todo    var emptySpawn = creep.pos.findClosest(FIND_MY_STRUCTURES, {filter: function(object) {return (object.structureType == STRUCTURE_EXTENSION) }});
	    if(creep.energy < creep.energyCapacity) {
            creep.moveTo(sources);
            creep.harvest(sources);
	    }
    	else {
    		creep.moveTo(Game.spawns.Spawn1);
    		creep.transferEnergy(Game.spawns.Spawn1)

    	}
    
    
}
